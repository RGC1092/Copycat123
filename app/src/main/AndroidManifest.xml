<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.evoqis.apps.manibhai">

    <application
        android:allowBackup="false"
        android:hardwareAccelerated="false"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Home"
            android:theme="@style/AppTheme"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Branches"
            android:configChanges="orientation"
            android:label="@string/branches"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Mandal"
            android:configChanges="orientation"
            android:label="@string/sanchalak_mandal"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Bank_Staff"
            android:configChanges="orientation"
            android:label="@string/bank_staff"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Calender"
            android:configChanges="orientation"
            android:label="@string/calender"
            android:screenOrientation="portrait" />
        <activity
            android:name=".FDRates"
            android:configChanges="orientation"
            android:label="@string/fd_rates"
            android:screenOrientation="portrait" />
        <activity
            android:name=".EmiCalc"
            android:configChanges="orientation"
            android:label="@string/emi_calc"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contact_info"
            android:configChanges="orientation"
            android:label="@string/contact_info"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contact_persons"
            android:configChanges="orientation"
            android:label="@string/contact_persons"
            android:screenOrientation="portrait" />
        <activity android:name=".ImageViewPager" />
        <activity
            android:name=".Others"
            android:configChanges="orientation"
            android:label="@string/locations"
            android:screenOrientation="portrait" />

    </application>

</manifest>